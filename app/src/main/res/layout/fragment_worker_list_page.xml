<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:backgroundTint="@color/light_white"
    tools:context=".worker_list_pageFragment">


    <TextView
        android:id="@+id/textView4"
        android:layout_width="32dp"
        android:layout_height="32dp"
        android:background="@drawable/ic_arrow_forward"
        android:backgroundTint="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/btn_back_medit2"
        app:layout_constraintEnd_toEndOf="@+id/btn_back_medit2"
        app:layout_constraintHorizontal_bias="0.895"
        app:layout_constraintStart_toStartOf="@+id/btn_back_medit2"
        app:layout_constraintTop_toTopOf="@+id/btn_back_medit2" />

    <View
        android:id="@+id/view3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/box_yellow_round_gradation"
        app:layout_constraintBottom_toTopOf="@+id/guideline32"
        app:layout_constraintEnd_toStartOf="@+id/guideline26"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/guideline20"
        app:layout_constraintTop_toTopOf="@+id/guideline31"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/btn_back_medit7"
        android:layout_width="133dp"
        android:layout_height="43dp"
        android:layout_marginStart="10dp"
        android:background="@drawable/button_round_blank"
        android:backgroundTint="@color/light_white"
        android:fontFamily="@font/sleigothic"
        android:onClick="onclick"
        android:paddingLeft="19dp"
        android:paddingTop="5dp"
        android:text="목록으로"
        android:textAlignment="viewEnd"
        android:textColor="@color/light_white"
        android:textSize="16sp"
        app:backgroundTint="@null"
        app:layout_constraintBottom_toBottomOf="@+id/tv_id"
        app:layout_constraintStart_toEndOf="@+id/tv_id"
        app:layout_constraintTop_toTopOf="@+id/tv_id"
        app:layout_constraintVertical_bias="0.55" />

    <TextView
        android:id="@+id/tv_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:backgroundTint="@color/seco"
        android:fontFamily="@font/sleigothic"
        android:gravity="center"
        android:text="김찬욱 근로자님"
        android:textColor="@color/seco"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/img"
        app:layout_constraintStart_toEndOf="@+id/img"
        app:layout_constraintTop_toTopOf="@+id/img" />

    <TextView
        android:id="@+id/tv_id2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/sleigothic"
        android:gravity="center"
        android:text="안전모 잠금 여부"
        android:textColor="@color/orange"
        android:textSize="20sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="57dp"
        tools:layout_editor_absoluteY="172dp" />

    <TextView
        android:id="@+id/tv_id3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/sleigothic"
        android:gravity="center"
        android:text="안전교육 수료 여부"
        android:textColor="@color/prim"
        android:textSize="20sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="211dp"
        tools:layout_editor_absoluteY="172dp" />

    <View
        android:id="@+id/view5"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/box_yellow_round_gradation_30dp"
        android:backgroundTint="@color/light_white"
        android:elevation="10dp"
        app:layout_constraintBottom_toTopOf="@+id/guideline46"
        app:layout_constraintEnd_toStartOf="@+id/guideline44"
        app:layout_constraintStart_toStartOf="@+id/guideline43"
        app:layout_constraintTop_toTopOf="@+id/guideline45"
        app:layout_constraintVertical_bias="0.413" />

    <Button
        android:id="@+id/btn_back_medit2"
        android:layout_width="175dp"
        android:layout_height="42dp"
        android:background="@drawable/button_round_blank"
        android:backgroundTint="@color/light_white"
        android:fontFamily="@font/sleigothic"
        android:onClick="onclick"
        android:paddingLeft="19dp"
        android:paddingTop="5dp"
        android:text="근로자정보수정"
        android:textAlignment="viewStart"
        android:textColor="@color/light_white"
        android:textSize="16sp"
        app:backgroundTint="@null"
        app:layout_constraintEnd_toStartOf="@+id/guideline26"
        app:layout_constraintStart_toStartOf="@+id/guideline20"
        app:layout_constraintTop_toTopOf="@+id/guideline41" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="28dp"
        android:layout_height="28dp"
        android:background="@drawable/edit"
        android:backgroundTint="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/btn_back_medit2"
        app:layout_constraintEnd_toEndOf="@+id/btn_back_medit2"
        app:layout_constraintHorizontal_bias="0.895"
        app:layout_constraintStart_toStartOf="@+id/btn_back_medit2"
        app:layout_constraintTop_toTopOf="@+id/btn_back_medit2" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="28dp"
        android:layout_height="28dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/sadface"
        android:backgroundTint="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/btn_back_medit5"
        app:layout_constraintEnd_toEndOf="@+id/btn_back_medit5"
        app:layout_constraintHorizontal_bias="0.91"
        app:layout_constraintStart_toStartOf="@+id/btn_back_medit5"
        app:layout_constraintTop_toTopOf="@+id/btn_back_medit5"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/btn_back_medit5"
        android:layout_width="175dp"
        android:layout_height="42dp"
        android:layout_marginTop="15dp"
        android:background="@drawable/button_round_blank"
        android:backgroundTint="@color/light_white"
        android:fontFamily="@font/sleigothic"
        android:onClick="onclick"
        android:paddingLeft="19dp"
        android:paddingTop="5dp"
        android:text="근로자삭제"
        android:textAlignment="viewStart"
        android:textColor="@color/light_white"
        android:textSize="16sp"
        app:backgroundTint="@null"
        app:layout_constraintEnd_toStartOf="@+id/guideline26"
        app:layout_constraintStart_toStartOf="@+id/guideline20"
        app:layout_constraintTop_toBottomOf="@+id/btn_back_medit2" />

    <ImageView
        android:id="@+id/img"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginStart="16dp"
        android:background="@drawable/icon_circle"
        android:backgroundTintMode="@null"
        app:layout_constraintBottom_toTopOf="@+id/guideline32"
        app:layout_constraintStart_toStartOf="@+id/guideline20"
        app:layout_constraintTop_toTopOf="@+id/guideline31"
        app:layout_constraintVertical_bias="0.023"
        tools:srcCompat="@drawable/worker_icon" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.03" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline43"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.06" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline44"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.94" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline26"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.97" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline31"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.03" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline45"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.2" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline46"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.5" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline32"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.7" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline41"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.55" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="32dp"
        android:layout_height="32dp"
        android:src="@drawable/ic_arrow_left"
        app:layout_constraintBottom_toBottomOf="@+id/btn_back_medit7"
        app:layout_constraintEnd_toEndOf="@+id/btn_back_medit7"
        app:layout_constraintHorizontal_bias="0.1"
        app:layout_constraintStart_toStartOf="@+id/btn_back_medit7"
        app:layout_constraintTop_toTopOf="@+id/btn_back_medit7" />

</androidx.constraintlayout.widget.ConstraintLayout>
