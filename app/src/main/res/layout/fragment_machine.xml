<?xml version="1.0" encoding="utf-8"?>


<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/prim"
    tools:context=".MachineFragment">


    <View
        android:id="@+id/view"
        android:layout_width="wrap_content"
        android:layout_height="609dp"
        android:background="@drawable/box_round"
        android:backgroundTint="@color/light_white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.666"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <EditText
        android:id="@+id/join_name"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:layout_marginTop="12dp"
        android:background="@drawable/button_round"
        android:backgroundTint="@color/white"
        android:ems="10"
        android:fontFamily="@font/sleigothic"
        android:hint="안전모ID를 입력하세요"
        android:inputType="textPersonName|textPassword"
        android:paddingLeft="10dp"
        app:layout_constraintEnd_toStartOf="@+id/guideline6"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/guideline4"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="281dp"
        android:layout_height="38dp"
        android:layout_marginTop="4dp"
        android:backgroundTint="@color/seco"
        android:fontFamily="@font/sleigothic"
        android:gravity="center"
        android:text="Machine resistration"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/guideline4"
        app:layout_constraintTop_toTopOf="@+id/guideline2" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="281dp"
        android:layout_height="38dp"
        android:layout_marginTop="36dp"
        android:backgroundTint="@color/seco"
        android:fontFamily="@font/sleigothic"
        android:gravity="center"
        android:text="기기 등록"
        android:textSize="24sp"
        app:layout_constraintEnd_toStartOf="@+id/guideline6"
        app:layout_constraintHorizontal_bias="0.466"
        app:layout_constraintStart_toStartOf="@+id/guideline4"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="281dp"
        android:layout_height="38dp"
        android:layout_marginTop="24dp"
        android:backgroundTint="@color/seco"
        android:fontFamily="@font/sleigothic"
        android:gravity="center"
        android:text="기기 정보 수정"
        android:textSize="24sp"
        app:layout_constraintEnd_toStartOf="@+id/guideline6"
        app:layout_constraintHorizontal_bias="0.466"
        app:layout_constraintStart_toStartOf="@+id/guideline4"
        app:layout_constraintTop_toBottomOf="@+id/btn_register" />

    <EditText
        android:id="@+id/w_id"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:layout_marginTop="12dp"
        android:background="@drawable/button_round"
        android:backgroundTint="@color/white"
        android:ems="10"
        android:fontFamily="@font/sleigothic"
        android:hint="근로자ID를 입력하세요"
        android:inputType="textPersonName|textPassword"
        android:paddingLeft="10dp"
        app:layout_constraintEnd_toStartOf="@+id/guideline6"
        app:layout_constraintHorizontal_bias="0.488"
        app:layout_constraintStart_toStartOf="@+id/guideline4"
        app:layout_constraintTop_toBottomOf="@+id/m_id" />

    <EditText
        android:id="@+id/m_id"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:layout_marginTop="84dp"
        android:background="@drawable/button_round"
        android:backgroundTint="@color/white"
        android:clickable="false"
        android:ems="10"
        android:fontFamily="@font/sleigothic"
        android:hint="안전모ID를 입력하세요"
        android:inputType="textPersonName"
        android:paddingLeft="10dp"
        app:layout_constraintEnd_toStartOf="@+id/guideline6"
        app:layout_constraintHorizontal_bias="0.488"
        app:layout_constraintStart_toStartOf="@+id/guideline4"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <Button
        android:id="@+id/btn_register"
        android:layout_width="196dp"
        android:layout_height="47dp"
        android:layout_marginTop="12dp"
        android:background="@drawable/button_round"
        android:backgroundTint="@color/seco"
        android:fontFamily="@font/sleigothic"
        android:onClick="onclick"
        android:text="기기 등록"
        android:textColor="#ffb606"
        android:textSize="16sp"
        app:backgroundTint="@null"
        app:layout_constraintEnd_toStartOf="@+id/guideline6"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="@+id/guideline4"
        app:layout_constraintTop_toBottomOf="@+id/w_id" />

    <Button
        android:id="@+id/btn_back_medit"
        android:layout_width="133dp"
        android:layout_height="43dp"
        android:layout_marginTop="36dp"
        android:background="@drawable/button_round"
        android:backgroundTint="@color/seco"
        android:fontFamily="@font/sleigothic"
        android:onClick="onclick"
        android:text="back"
        android:textColor="#ffb606"
        android:textSize="16sp"
        app:backgroundTint="@null"
        app:layout_constraintEnd_toStartOf="@+id/guideline6"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="@+id/guideline4"
        app:layout_constraintTop_toBottomOf="@+id/btn_modify" />

    <Button
        android:id="@+id/btn_modify"
        android:layout_width="197dp"
        android:layout_height="45dp"
        android:layout_marginTop="12dp"
        android:background="@drawable/button_round"
        android:backgroundTint="@color/seco"
        android:fontFamily="@font/sleigothic"
        android:onClick="onclick"
        android:text="기기 수정"
        android:textColor="#ffb606"
        android:textSize="16sp"
        app:backgroundTint="@null"
        app:layout_constraintEnd_toStartOf="@+id/guideline6"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="@+id/guideline4"
        app:layout_constraintTop_toBottomOf="@+id/join_tel" />

    <EditText
        android:id="@+id/join_tel"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/button_round"
        android:backgroundTint="@color/white"
        android:ems="10"
        android:fontFamily="@font/sleigothic"
        android:hint="근로자ID를 입력하세요"
        android:inputType="textPersonName|textPassword"
        android:paddingLeft="10dp"
        app:layout_constraintEnd_toStartOf="@+id/guideline6"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/guideline4"
        app:layout_constraintTop_toBottomOf="@+id/join_name" />


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="59dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="653dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="58dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="354dp" />


</androidx.constraintlayout.widget.ConstraintLayout>









